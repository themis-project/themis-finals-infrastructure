{
  "environment": "development",
  "run_list": [
    "recipe[latest-git::default]",
    "recipe[themis-finals::python2]",
    "recipe[dotfiles::default]",
    "recipe[themis-finals-service1-checker::default]"
  ],
  "dotfiles": {
    "users": [
      "root",
      "vagrant"
    ]
  },
  "rbenv": {
    "group_users": [
      "root",
      "vagrant"
    ]
  },
  "latest-redis": {
    "listen": {
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "themis-finals-service1-checker": {
    "fqdn": "service1.checker.finals.themis-project.com",
    "debug": true,
    "autostart": true
  },
  "supervisor": {
    "inet_port": "0.0.0.0:9001"
  },
  "automatic": {
    "ipaddress": "service1.checker.finals.themis-project.com"
  }
}
